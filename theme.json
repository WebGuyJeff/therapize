{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"customTemplates": [
		{ "name": "page-full-width", "title": "Page: Full-width", "postTypes": [ "page" ] },
		{ "name": "page-no-header", "title": "Page: No Header", "postTypes": [ "page" ] },
		{ "name": "page-no-title", "title": "Page: No Title", "postTypes": [ "page" ] },
		{ "name": "page-with-hero", "title": "Page: Hero Banner", "postTypes": [ "page" ] },
		{ "name": "archive-service", "title": "Archive: Service Posts" },
		{ "name": "archive-review", "title": "Archive: Review Posts" },
		{ "name": "category-partners", "title": "Category: Partners" },
		{ "name": "single-review", "title": "Single: Review", "postTypes": [ "review" ] },
		{ "name": "single-service", "title": "Single: Service", "postTypes": [ "service" ] },
		{ "name": "single-partners", "title": "Single: Partners Category", "postTypes": [ "post" ] }
	],
	"templateParts": [
		{ "name": "header", "title": "Header", "area": "header" },
		{ "name": "footer", "title": "Footer", "area": "footer" }
	],
	"settings": {
		"appearanceTools": true,
		"background": {
			"backgroundImage": false,
			"backgroundSize": false
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"text": true,
			"background": true,
			"link": true,
			"heading": true,
			"button": true,
			"caption": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": ["#111111", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				},
				{
					"colors": ["#111111", "#C2A990"],
					"slug": "duotone-2",
					"name": "Black and sandstone"
				},
				{
					"colors": ["#111111", "#D8613C"],
					"slug": "duotone-3",
					"name": "Black and rust"
				},
				{
					"colors": ["#111111", "#B1C5A4"],
					"slug": "duotone-4",
					"name": "Black and sage"
				},
				{
					"colors": ["#111111", "#B5BDBC"],
					"slug": "duotone-5",
					"name": "Black and pastel blue"
				}
			],
			"gradients": [
                {
                    "slug": "radial-light-green-white",
                    "gradient": "radial-gradient(circle at 100% 100%, var(--wp--preset--color--theme-green-light) 0%, var(--wp--preset--color--theme-white) 140%)",
                    "name": "Light Green to White (radial)"
                },
				{
                    "slug": "radial-green-light-green",
                    "gradient": "radial-gradient(circle at 100% 100%, var(--wp--preset--color--theme-green) -40%, var(--wp--preset--color--theme-green-light) 100%)",
                    "name": "Green to Light Green (radial)"
                },
				{
                    "slug": "radial-green-white",
                    "gradient": "radial-gradient(circle at 100% 100%, var(--wp--preset--color--theme-green) -40%, var(--wp--preset--color--theme-white) 140%)",
                    "name": "Green to White (radial)"
                },
				{
                    "slug": "linear-white-light-green",
                    "gradient": "linear-gradient(180deg, var(--wp--preset--color--theme-white) -40%, var(--wp--preset--color--theme-green-light) 140%)",
                    "name": "White to Light Green (linear)"
                },
				{
                    "slug": "linear-light-green-green",
                    "gradient": "linear-gradient(180deg, var(--wp--preset--color--theme-green-light) 0%, var(--wp--preset--color--theme-green) 100%)",
                    "name": "Light Green to Green (linear)"
                },
				{
                    "slug": "linear-white-green",
                    "gradient": "linear-gradient(180deg, var(--wp--preset--color--theme-white) -20%, var(--wp--preset--color--theme-green) 100%)",
                    "name": "White to Green (linear)"
                },
				{
					"slug": "green-stripes",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--theme-green-dark) 33%, var(--wp--preset--color--theme-green) 33.001%, var(--wp--preset--color--theme-green) 65.999%, var(--wp--preset--color--theme-green-light) 66%)",
					"name": "Green Stripes (linear)"
				},
				{
					"slug": "green-fade",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--theme-green-dark) 0%, var(--wp--preset--color--theme-green-light) 50%, var(--wp--preset--color--theme-green) 100%)",
					"name": "Green Fade (linear)"
				},
				{
					"slug": "green-fade-45",
					"gradient": "linear-gradient(45deg, var(--wp--preset--color--theme-green-dark) 0%, var(--wp--preset--color--theme-green-light) 50%, var(--wp--preset--color--theme-green) 100%)",
					"name": "Green Fade Alt (linear)"
				},
				{
					"slug": "green-fade-225",
					"gradient": "linear-gradient(225deg, var(--wp--preset--color--theme-green-dark) 0%, var(--wp--preset--color--theme-green-light) 50%, var(--wp--preset--color--theme-green) 100%)",
					"name": "Green Fade Alt 2 (linear)"
				}
			],
			"palette": [
				{ "slug": "theme-white","color":"#f2f3ef","name":"White"},
				{ "slug": "theme-black","color":"#323b3b","name":"Black"},
				{ "slug": "theme-green-dark","color":"#33786b","name":"Dark Green"},
				{ "slug": "theme-green","color":"#49b697","name":"Green"},
				{ "slug": "theme-green-light","color":"#90d0ba","name":"Light Green"}
			]
		},
		"dimensions": {
			"minHeight": true
		},
		"layout": {
			"contentSize": "38rem",
			"wideSize": "70rem",
			"allowEditing": true,
			"allowCustomContentAndWideSize": true
		},
		"position": {
			"sticky": true
		},
		"spacing": {
			"customSpacingSize": true,
			"defaultSpacingSizes": false,
			"padding": true,
			"margin": true,
			"blockGap": true,
			"units": [ "em", "rem", "vh", "vw", "%" ],
			"spacingSizes": [
				{ "name": "1 (0.5rem)", "slug": "20", "size": "0.5rem" },
				{ "name": "2 (1rem)", "slug": "30", "size": "1rem" },
				{ "name": "3 (2rem)", "slug": "40", "size": "2rem" },
				{ "name": "4 (3.5rem)", "slug": "50", "size": "3rem" },
				{ "name": "5 (5.5rem)", "slug": "60", "size": "5rem" },
				{ "name": "6 (8rem)", "slug": "70", "size": "8rem" },
				{ "name": "7 (11rem)", "slug": "80", "size": "12rem" }
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{ "name": "Tight", "slug": "theme-tight", "shadow": "0 0.08rem 0.1em 0 #000, 0 0.15rem 0.3rem 0.15rem #0004" },
				{ "name": "Small", "slug": "theme", "shadow": "0 0.25rem 0.5em 0.25rem #0006,0 0.125rem 0.25rem 0.125rem #0006" },
				{ "name": "Soft", "slug": "theme-soft", "shadow": "0 1px 1px rgba(0,0,0,0.12), 0 2px 2px rgba(0,0,0,0.12), 0 4px 4px rgba(0,0,0,0.12), 0 8px 8px rgba(0,0,0,0.12), 0 16px 16px rgba(0,0,0,0.12)" },
				{ "name": "Soft Upward", "slug": "theme-soft-up", "shadow": "0 -1px 1px rgba(0,0,0,0.12), 0 -2px 2px rgba(0,0,0,0.12), 0 -4px 4px rgba(0,0,0,0.12), 0 -8px 8px rgba(0,0,0,0.12), 0 -16px 16px rgba(0,0,0,0.12)" },
				{ "name": "Inset", "slug": "theme-inset", "shadow": "inset -0.1rem 0.2rem 0.5rem 0.2rem #000c" },
				{ "name": "Hard Offset Small", "slug": "hard-offset-small", "shadow": "0.25rem 0.25rem 0 var(--wp--preset--color--theme-green)" },
				{ "name": "Hard Offset Large", "slug": "hard-offset-large", "shadow": "0.5rem 0.5rem 0 var(--wp--preset--color--theme-green)" }
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"dropCap": true,
			"fontStyle": true,
			"fontWeight": true,
			"fluid": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textColumns": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter, Arial, sans-serif",
					"name": "Inter",
					"slug": "inter",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Inter/inter.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "oblique 0deg 9.4deg",
							"fontWeight": "100 900",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Inter/inter.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'Open Sans', Arial, sans-serif",
					"name": "Open Sans",
					"slug": "open-sans",
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 800",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Open_Sans/open-sans.woff2"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300 800",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Open_Sans/open-sans-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'Comfortaa', Arial, sans-serif",
					"name": "Comfortaa",
					"slug": "comfortaa",
					"fontFace": [
						{
							"fontFamily": "Comfortaa",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 700",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Comfortaa/comfortaa.woff2"
							]
						},
						{
							"fontFamily": "Comfortaa",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300 700",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Comfortaa/comfortaa.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{ "name": "X-Small", "size": "clamp(0.8889rem, 0.8761rem + 0.0512vw, 0.9375rem)", "slug": "x-small" },
				{ "name": "Small Base", "size": "clamp(1rem, 0.9671rem + 0.1316vw, 1.125rem)", "slug": "small" },
				{ "name": "Medium H6", "size": "clamp(1.125rem, 1.0658rem + 0.2368vw, 1.35rem)", "slug": "medium" },
				{ "name": "Large H5", "size": "clamp(1.2656rem, 1.1724rem + 0.373vw, 1.62rem)", "slug": "large" },
				{ "name": "X-Large H4", "size": "clamp(1.4238rem, 1.2869rem + 0.5475vw, 1.944rem)", "slug": "x-large" },
				{ "name": "XX-Large H3", "size": "clamp(1.6018rem, 1.4094rem + 0.7695vw, 2.3328rem)", "slug": "xx-large" },
				{ "name": "XXX-Large H2", "size": "clamp(1.802rem, 1.5396rem + 1.0498vw, 2.7994rem)", "slug": "xxx-large" },
				{ "name": "XXXX-Large H1", "size": "clamp(2.0273rem, 1.6768rem + 1.402vw, 3.3592rem)", "slug": "xxxx-large" },
				{ "name": "Display Small", "size": "clamp(2.2807rem, 1.8201rem + 1.8425vw, 4.0311rem)", "slug": "display-small" },
				{ "name": "Display Medium", "size": "clamp(2.5658rem, 1.968rem + 2.3911vw, 4.8373rem)", "slug": "display-medium" },
				{ "name": "Display Large", "size": "clamp(2.8865rem, 2.1185rem + 3.0718vw, 5.8048rem)", "slug": "display-large" },
				{ "name": "Display X-Large", "size": "clamp(3.2473rem, 2.2688rem + 3.9141vw, 6.9657rem)", "slug": "display-x-large" }
			]
		},
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"gutter": {
				"small": "var( --wp--preset--spacing--40 )",
				"medium": "clamp( var( --wp--preset--spacing--50 ), 4vw, var( --wp--preset--spacing--60 ) )",
				"large": "var( --wp--preset--spacing--70 )",
				"x-large": "var( --wp--preset--spacing--80 )"
			},
			"border-size": "0.1875rem",
			"border-radius": {
				"small": "0.5rem",
				"medium": "1rem",
				"large": "2rem"
			},
			"padding-page": "var( --wp--custom--gutter--medium )",
			"transition": {
				"hover-time": "0.15s"
			}
		},
		"blocks": {
			"core/group": {
				"spacing": {
					"margin": true,
					"padding": true,
					"blockGap": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--theme-white)",
			"text": "var(--wp--preset--color--theme-black)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontWeight": "400",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)",
			"padding": {
				"top": "0",
				"bottom": "0",
				"right": "var(--wp--custom--padding-page)",
				"left": "var(--wp--custom--padding-page)"
			}
		},
		"elements": {
			"button": {
				"color": { "background": "var(--wp--preset--color--theme-black)", "text": "var(--wp--preset--color--theme-white)" }
			},
			"link": { "color": { "text": "inherit" } },
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--comfortaa)",
					"fontWeight": "700",
					"lineHeight": "1.3"
				}
			},
			"h1": { "typography": { "fontSize": "var(--wp--preset--font-size--xxxx-large)" } },
			"h2": { "typography": { "fontSize": "var(--wp--preset--font-size--xxx-large)" } },
			"h3": { "typography": { "fontSize": "var(--wp--preset--font-size--xx-large)" } },
			"h4": { "typography": { "fontSize": "var(--wp--preset--font-size--x-large)" } },
			"h5": { "typography": { "fontSize": "var(--wp--preset--font-size--large)" } },
			"h6": { "typography": { "fontSize": "var(--wp--preset--font-size--medium)" } }
		},
		"blocks": {
			"core/buttons": { "spacing": { "blockGap": "var(--wp--custom--gutter--small)" } },
			"core/button": {
				"typography": {
					"lineHeight": "1",
					"fontWeight": "600"
				},
				"color": { "background": "var(--wp--preset--color--theme-black)", "text": "var(--wp--preset--color--theme-white)" },
				"border": { "radius": "var(--wp--custom--border-radius--small)", "width": "0" },
				"variations": {
					"outline": {
						"color": { "background": "#0000", "text": "currentColor" },
						"border": { "width": "var(--wp--custom--border-size)", "color": "currentColor" }
					}
				}
			},
			"core/comment-author-name": { "typography": { "fontSize": "var(--wp--preset--font-size--medium)" } },
			"core/comment-content": {
				"border": { "radius": "var(--wp--custom--border-radius--small)" },
				"color": { "background": "var(--wp--preset--color--theme-black)" }
			},
			"core/comment-date": {
				"typography": {
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": { "textTransform": "uppercase" },
				"elements": {
					"link": {
						"typography": { "fontSize": "var(--wp--preset--font-size--small)" },
						"color": { "text": "var(--wp--preset--color--theme-green-dark)" }
					}
				}
			},
			"core/group": {
				"spacing": { "blockGap": "var(--wp--preset--spacing--50)" }
			},
			"core/columns": {
				"spacing": { "blockGap": "var(--wp--preset--spacing--50)" }
			},
			"core/post-template": {
				"spacing": { "blockGap": "var(--wp--preset--spacing--50)" }
			},
			"core/navigation": { "spacing": { "blockGap": "var(--wp--custom--gutter--small)" } },
			"core/quote": { "typography": { "fontSize": "var(--wp--preset--font-size--x-large)" } },
			"core/search": { "border": { "radius": "var(--wp--custom--border-radius--small)" } },
			"core/code": {
				"typography": { "fontFamily": "monospace" },
				"color": { "text": "#8ebff5", "background": "#222" },
				"spacing": { "padding": { "bottom": "var(--wp--preset--spacing--50)", "left": "var(--wp--preset--spacing--30)", "right": "var(--wp--preset--spacing--30)", "top": "var(--wp--preset--spacing--50)" } },
				"shadow": "var(--wp--preset--shadow--theme-inset)"
			},
			"core/table": {
				"css": "border-color:var(--wp--preset--color--theme-black); border-bottom: solid 0.25rem; & th{ color:var(--wp--preset--color--theme-white); background:var(--wp--preset--color--theme-black); border: 0px; } & td{ border: 0px; } & tr:nth-child(even){ background: #fff1; }",
				"color": { "text": "var(--wp--preset--color--theme-black)" }
			}
		}
	}
}
